[MASTER]
init-hook='import os; venv = os.getenv('VIRTUAL_ENV', ''); act = os.path.join(venv, 'bin', 'activate_this.py'); exec_str = "if venv != '': exec(open(act).read(), {'__file__': act})"; exec(exec_str)'

[MESSAGES CONTROL]
disable=
	no-member,
	attribute-defined-outside-init,
	duplicate-code,
	fixme,
	missing-docstring,
	protected-access,
	invalid-name,
	too-few-public-methods,
	too-many-statements,
	too-many-branches,
	too-many-locals

[FORMAT]
max-line-length=256
